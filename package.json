    "gatsby-source-prismic-graphql": "3.4.0-beta.1",
    "gatsby-transformer-json": "^2.2.13",
    "gatsby-transformer-sharp": "^2.2.21",
    "less": "^3.10.3",
    "prettier": "^1.18.2"
  }
