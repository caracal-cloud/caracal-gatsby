{
  "name": "caracal-site",
  "private": true,
  "description": "Caracal, geospatial data integration static public site.",
  "version": "0.1.0",
  "author": "Casey Slaught <casey@caracal.cloud>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy": "aws --profile caracal-admin s3 sync public/ s3://caracal-site && aws --profile caracal-admin cloudfront create-invalidation --distribution-id E209BRVNAT7TTU --paths '/*'"
  },
