    "gatsby-image": "^2.2.25",
    "lodash": "^4.17.20",
    "match-sorter": "^4.0.2",
    "prismic-reactjs": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "styled-tools": "^1.7.1",
    "xstate": "^4.6.7",
    "yup": "^0.27.0"
  },
